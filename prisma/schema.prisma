generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bot {
  id Int @default(autoincrement())
  tgid String @unique
  confession String?
  chatId String?
  channelId String?
  type Type
  code String
}

enum Type {
  TAKES
  MP
  MOD
}
